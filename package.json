{
  "name": "roulette",
  "version": "0.0.1",
  "main": "src/index.ts",
  "license": "UNLICENSED",
  "private": true,
  "scripts": {
    "start": "npm run build",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "dev": "vite",
    "build": "vite build",
    "lint": "bunx prettier@latest --write .",
    "precommit": "npm run lint",
    "prepush": "npm run lint && npm run test"
  },
  "jest": {
    "coverageDirectory": "./coverage",
    "collectCoverageFrom": [
      "src/**/*",
      "!src/**/*.d.ts",
      "!src/**/*.spec.ts"
    ],
    "coverageReporters": [
      "json",
      "lcov",
      "text"
    ],
    "moduleFileExtensions": [
      "ts",
      "js"
    ],
    "transform": {
      "\\.(ts|tsx)$": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    },
    "testRegex": ".*\\.spec.(ts|tsx|js)$"
  },
  "dependencies": {},
  "devDependencies": {
    "@happy-dom/global-registrator": "^11.0.2",
    "@tweenjs/tween.js": "^17.2.0",
    "@types/jest": "^22.2.0",
    "@types/node": "^9.6.0",
    "@types/three": "^0.89.11",
    "@types/tween.js": "^16.9.0",
    "coveralls": "^3.0.0",
    "cross-env": "^5.1.3",
    "eslint": "^4.18.2",
    "husky": "^0.14.3",
    "jest": "^22.4.2",
    "sass": "^1.66.1",
    "three": "^0.91.0",
    "three-collada-loader": "^0.0.1",
    "ts-jest": "^22.4.1",
    "typescript": "^2.7.2",
    "vite": "^4.4.9"
  }
}
